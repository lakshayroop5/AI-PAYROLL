# Database Configuration
DATABASE_URL="file:./dev.db"

# NextAuth.js Configuration
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-secret-key-here"

# GitHub OAuth Configuration
GITHUB_CLIENT_ID="your-github-client-id"
GITHUB_CLIENT_SECRET="your-github-client-secret"
GITHUB_TOKEN="your-github-personal-access-token"

# Self Network Configuration
SELF_APP_ID="your-self-app-id"
SELF_APP_SECRET="your-self-app-secret"

# The Graph Protocol Configuration
GITHUB_SUBGRAPH_URL="https://api.thegraph.com/subgraphs/name/your-subgraph"
GRAPH_API_KEY="your-graph-api-key"

# Fetch.ai / ASI Configuration
FETCHAI_AGENT_ENDPOINT="https://agentverse.ai/v1/agents"
FETCHAI_API_KEY="your-fetchai-api-key"

# Hedera Network Configuration
HEDERA_NETWORK="testnet"
HEDERA_OPERATOR_ACCOUNT_ID="0.0.12345"
HEDERA_OPERATOR_PRIVATE_KEY="your-hedera-private-key"
HEDERA_TREASURY_ACCOUNT_ID="0.0.67890"

# Lighthouse Web3 Storage Configuration
LIGHTHOUSE_API_KEY="your-lighthouse-api-key"

# SendGrid Email Configuration
SENDGRID_API_KEY="your-sendgrid-api-key"
SENDGRID_FROM_EMAIL="noreply@your-domain.com"
ADMIN_EMAIL="admin@your-domain.com"

# Pyth Network Configuration (for price feeds)
PYTH_ENDPOINT="https://hermes.pyth.network"
PYTH_HERMES_URL="https://hermes.pyth.network"
MAX_PRICE_STALENESS_SECONDS="300"
DEFAULT_RETRY_ATTEMPTS="3"

# Application Configuration
NODE_ENV="development"
PORT=3000

# Webhook Configuration (for external integrations)
AGENT_WEBHOOK_URL="http://localhost:3001/webhooks/agent-setup"

# Database URLs for different environments
# Production example:
# DATABASE_URL="postgresql://username:password@hostname:port/database"

# Additional Configuration
CORS_ORIGIN="http://localhost:3000"
LOG_LEVEL="info"
